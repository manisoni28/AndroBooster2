        <style name="GamerTheme" parent="Theme.AppCompat.Light.NoActionBar">
                <!-- Customize your theme here. -->
                <item name="colorPrimary">@color/gamer_green</item>
                <item name="android:navigationBarColor">@color/gamer_green_dark</item>
                <item name="colorPrimaryDark">@color/gamer_green_dark</item>
                <item name="colorAccent">@color/gamer_green</item>
        </style>
</resources>
